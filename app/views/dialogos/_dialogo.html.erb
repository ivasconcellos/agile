<div id=modulo_ordenacao_<%= modulo_ordenacao.id %>>        
    <%= link_to modulo_ordenacao.objeto do %>
      <i class = "fa fa-commenting-o text-dark"> <%= modulo_ordenacao.objeto.nome %></i>
    <% end %>
	<% if can? :edit, Dialogo %>
	<%= link_to '', edit_dialogo_path(modulo_ordenacao.objeto), class: "fa fa-edit" %>
	<% end %>
	<% if can? :destroy, Dialogo %>
	<%= link_to '', modulo_ordenacao.objeto, method: :delete, data: { confirm: 'Deseja realmente excluir o Dialogo?' }, class: "fa fa-trash" %>
	<% end %>
</div>