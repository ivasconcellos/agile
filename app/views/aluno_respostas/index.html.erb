<p id="notice"><%= notice %></p>

<h1>Aluno Respostas</h1>

<table>
  <thead>
    <tr>
      <th>Usuario curso</th>
      <th>Respostas perguntas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aluno_respostas.each do |aluno_resposta| %>
      <tr>
        <td><%= aluno_resposta.usuario_curso %></td>
        <td><%= aluno_resposta.resposta_pergunta %></td>
        <td><%= link_to 'Show', aluno_resposta %></td>
        <td><%= link_to 'Edit', edit_aluno_resposta_path(aluno_resposta) %></td>
        <td><%= link_to 'Destroy', aluno_resposta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aluno Resposta', new_aluno_resposta_path %>
