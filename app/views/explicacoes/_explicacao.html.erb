<div id=modulo_ordenacao_<%= modulo_ordenacao.id %> class="modulo-item">   
    <%= link_to modulo_ordenacao.objeto do %>
      <i class = "fa fa-lightbulb-o text-orange"> <%= modulo_ordenacao.objeto.nome %></i>
    <% end %>
	<% if can? :edit, Explicacao %>
	<%= link_to '', edit_explicacao_path(modulo_ordenacao.objeto), class: "fa fa-edit" %>
	<% end %>
	<% if can? :destroy, Explicacao %>
	<%= link_to '', modulo_ordenacao.objeto, method: :delete, data: { confirm: 'Deseja realmente excluir a Explicação?' }, class: "fa fa-trash" %>
	<% end %>
</div>
    
